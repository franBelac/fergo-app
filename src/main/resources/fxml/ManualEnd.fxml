<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="457.0" prefWidth="606.0" style="-fx-background-color: rgb(65,65,65);" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.ManualEndController">
   <children>
      <Label alignment="CENTER" layoutX="244.0" layoutY="59.0" text="Finished!" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutX="94.0" layoutY="155.0" prefHeight="54.0" prefWidth="418.0" text="Click to save the rac files to your set path" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="18.0" />
         </font>
      </Label>
      <Button fx:id="generateBtn" alignment="CENTER" layoutX="244.0" layoutY="371.0" mnemonicParsing="false" onAction="#clickGenerate" prefHeight="71.0" prefWidth="119.0" text="Click this">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font></Button>
      <Label fx:id="confirmLabel" layoutX="268.0" layoutY="328.0" prefHeight="26.0" prefWidth="70.0" text="Success!" textFill="#00ff44">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font>
      </Label>
      <Label fx:id="warningEndLabel" layoutX="134.0" layoutY="200.0" text="Please set the file destination for your rac files first" textFill="#ff2323">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
